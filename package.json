{
  "name": "slack-stock",
  "version": "0.1.0",
  "bin": {
    "slst": "./bin/run"
  },
  "bugs": "https://github.com/cli/slack-stock/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@prisma/client": "^3.4.1",
    "@types/better-sqlite3": "^7.4.0",
    "@types/chai": "^4.2.22",
    "@types/inquirer": "^8.1.3",
    "@types/inquirer-autocomplete-prompt": "^1.3.3",
    "@types/js-yaml": "^4.0.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "@types/papaparse": "^5.3.1",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "prisma": "^3.4.1",
    "ts-node": "^8.10.2",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/cli/slack-stock",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "slst",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "cli/slack-stock",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "start": "./bin/run",
    "migrate": "prisma migrate dev",
    "seed": "prisma db seed"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.3",
    "@slack/web-api": "^6.4.0",
    "better-sqlite3-helper": "^3.1.5",
    "cli-ux": "^5.6.3",
    "dayjs": "^1.10.7",
    "inquirer": "^8.2.0",
    "inquirer-autocomplete-prompt": "^1.4.0",
    "inquirer-date-prompt": "^2.0.1",
    "kleur": "^4.1.4",
    "papaparse": "^5.3.1",
    "tslib": "^1.14.1",
    "xlsx": "^0.17.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
